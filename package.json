{
  "name": "frictionless-ui",
  "version": "0.1.0",
  "license": "MIT",
  "author": "roll <eskarev@gmail.com>",
  "description": "UI components for frictionless",
  "homepage": "https://github.com/frictionlessdata/frictionless-ui",
  "bugs": "https://github.com/frictionlessdata/frictionless-ui/issues",
  "repository": "github:frictionlessdata/frictionless-ui",
  "main": "dist/frictionless-ui.js",
  "engines": {
    "node": ">=4"
  },
  "files": [
    "dist",
    "lib",
    "src"
  ],
  "scripts": {
    "analyze": "NODE_ENV=production webpack --profile --json | webpack-bundle-size-analyzer",
    "build": "npm run compile && npm run bundle",
    "bundle": "webpack --output-pathinfo --progress --hide-modules && NODE_ENV=production webpack --progress --hide-modules",
    "compile": "rm -rf lib && tsc && cp -r src/images lib && cp -r src/styles lib",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "start": "webpack-dev-server --inline --hot --open",
    "e2e": "cypress run",
    "format": "prettier --write 'src/**/*.ts*' && eslint --fix 'src/**/*.ts*'",
    "lint": "prettier --check 'src/**/*.ts*' && eslint 'src/**/*.ts*'",
    "pretest": "npm run lint",
    "readme": "doctoc --maxlevel 3 README.md",
    "test": "jest --coverage",
    "update": "ncu -u"
  },
  "dependencies": {
    "@types/classnames": "^2.2.10",
    "@types/lodash": "^4.14.149",
    "@types/marked": "^0.7.4",
    "classnames": "^2.2.6",
    "hex-to-rgba": "^2.0.1",
    "jsonschema": "^1.2.6",
    "lodash": "^4.17.15",
    "marked": "^0.8.2"
  },
  "peerDependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.6",
    "@testing-library/jest-dom": "^5.5.0",
    "@testing-library/react": "^10.0.2",
    "@types/jest": "^25.2.1",
    "@types/node-fetch": "^2.5.6",
    "@typescript-eslint/eslint-plugin": "^2.28.0",
    "@typescript-eslint/parser": "^2.28.0",
    "compression-webpack-plugin": "^3.1.0",
    "coveralls": "^3.0.11",
    "css-loader": "^3.5.1",
    "cypress": "^4.4.0",
    "doctoc": "^1.4.0",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-cypress": "^2.10.3",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-standard": "^4.0.1",
    "file-loader": "^6.0.0",
    "http-server": "^0.12.1",
    "husky": "^4.2.3",
    "jest": "^25.3.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-fetch": "^2.6.0",
    "node-sass": "^4.13.1",
    "npm-check-updates": "^4.1.2",
    "prettier": "^2.0.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-hot-loader": "^4.12.20",
    "react-addons-test-utils": "^15.6.2",
    "react-test-renderer": "^16.13.1",
    "referencer": "^0.2.5",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "ts-jest": "^25.3.1",
    "ts-loader": "^6.2.2",
    "typescript": "^3.8.3",
    "webpack": "^4.42.1",
    "webpack-bundle-size-analyzer": "^3.1.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  }
}
